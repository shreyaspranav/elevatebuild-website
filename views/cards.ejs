    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 row-cols-xs-1 card-cell">
        <% for(let p of prop) { %>
        <div class="card" style="width: 20rem">
            <a href="/home/card/<%=p._id%>" class="card-link">
                <img
                    src="<%=p.image %>"
                    class="card-img-top"
                    alt="pet-image"
                    style="height: 15rem"
                />
                <div class="card-body">
                    <div class="card-img-overlay"></div>
                    <h5 class="card-title text-center"><%= p.title %></h5>
                    <p class="card-text text-center">
                        <%= p.address%>
                    </p>
                    <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
                </div>
            </a>
        </div>
        <% } %>
    </div>

